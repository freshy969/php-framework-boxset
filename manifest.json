{
	"minPhpVersion": "5.0",
	"url": {
		"domain": "{domain}",
		"root": "{url_root}"
	},
	"session": {
		"token": "APP_TOKEN",
		"encrypt": "{encrypt-key}"
	},
	"application": {
		"activity": {
			"main": {
				"launch": "MainApp",
				"storage": "{file-storage-path}",
				"languageSource": "test_db",
				"language": [
					"zt",
					"zs",
					"en"
				]
			}
		}
	},
	"database": {
		"test_db": {
			"host": "{sql-host}",
			"user": "{user-name}",
			"password": "{user-password}",
			"db_name": "{database-name}"
		}
	}
}
